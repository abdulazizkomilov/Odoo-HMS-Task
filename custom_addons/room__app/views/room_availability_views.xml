<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="room_availability_tree_view" model="ir.ui.view">
        <field name="name">Room Availability Tree View</field>
        <field name="model">room__app.room_availability</field>
        <field name="arch" type="xml">
            <tree>
                <field name="room_id"/>
                <field name="date"/>
                <field name="is_available"/>
            </tree>
        </field>
    </record>
</odoo>
